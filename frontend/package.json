{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --port 3000",
    "lint": "yarn lint-coffeescript && yarn lint-dependencies && yarn lint-stylus",
    "lint-coffeescript": "coffeelint -q src",
    "lint-dependencies": "dependency-lint",
    "lint-stylus": "./node_modules/.bin/stylint src/",
    "test": "yarn lint && yarn unit-test",
    "unit-test": "mocha 'src/**/*_test.coffee'"

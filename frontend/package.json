    "coffeescope2": "^0.4.5",
    "dependency-lint": "^4.1.4",
    "enzyme": "^2.4.1",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",

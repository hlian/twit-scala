    "css-loader": "^0.25.0",
    "html-webpack-plugin": "^2.22.0",
    "i18n-js": "^1.0.0",
    "lodash": "^4.16.4",
    "postcss-loader": "^1.2.2",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-e": "^2.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.7.0",
